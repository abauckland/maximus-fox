<div id="project_info">  <div id="current_project_title">       <%= @current_project.project_code_and_title %>  </div>  <div id="project_status">    Status: <%= @current_project.project_status %>  </div>  <div id="project_rev">    Rev: <%= @current_revision_rev %>  </div></div><div id="option_band">  <div id="option_select_1">Revision                                     <%= print_revision_select_list(@current_project, @project_revisions, @selected_revision)%>  </div></div><div id="edit_box_less">  <%= print_help(@current_project)%>  </div><form action="<%= print_project_print_path %>" method="get" >  <%= hidden_field_tag "id", @current_project.id %>  <%= hidden_field_tag "revision", @selected_revision.id %>  <div id="watermark_check">    <%= watermark_checkbox(@current_project, @last_project_revision, @selected_revision) %>  </div>  <div id="bottom_button">    <%= submit_tag 'Print Document'%>  </div></form>