    <!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"        "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">      <html>        <head>           <%= stylesheet_link_tag 'specright-mobile' %>          <%= javascript_include_tag   'jquery', 'rails.js',  'jquery-ui-1.8.9.custom.min', 'jquery.jeditable', 'jquery.idTabs.min', 'jquery.tipsy', 'autocomplete-rails.js', 'application' %>       </head>        <body>   <div class="current">        <div class="toolbar">       <%= link_to "Back", projects_path, :class => "back", :rel => "external" %>      <h1><%= @current_project.code %> rev <%= @current_revision_rev %></h1>      </div>        <% unless flash.empty? %>        <div class="info">        <%- flash.each do |name, msg| -%>          <%= content_tag :div, msg, :id => "flash_#{name}" %>        <%- end -%>        </div>      <% end %>   <ul>  <li class="sep">Print Revision:</li>  <li><%= revision_select_list(@current_project, @project_revisions, @selected_revision)%></li>                            </ul><form action="<%= print_project_print_path %>" method="get" >  <%= hidden_field_tag "id", @current_project.id %>  <%= hidden_field_tag "revision", @selected_revision.id %>  <ul>    <li class="sep"><%= watermark_checkbox(@current_project, @all_project_revisions, @selected_revision) %></li>  </ul><%= submit_tag "Print Document" %></form>    </div>     </body>  </html