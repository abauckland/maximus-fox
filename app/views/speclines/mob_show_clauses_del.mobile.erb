   <div class="current">      <div id="toolbar">       <%= link_to "Back", {:controller => "projects", :action => "show", :id => @current_project.id, :subsection => @current_subsection_id}, :class => "back", :rel => "external" %>      <h1>delete clause</h1>      <%= link_to "Finish", {:controller => "projects", :action => "show", :id => @current_project.id, :subsection => @current_subsection_id}, :class => "del", :rel => "external" %>       </div>        <% unless flash.empty? %>        <div class="info">        <%- flash.each do |name, msg| -%>          <%= content_tag :div, msg, :id => "flash_#{name}" %>        <%- end -%>        </div>      <% end %>      <ul>  <%@current_subsection_clauses.each do |clause| %>  <li  id='<%=clause.id%>' class="del_arrow"><%= mob_delete_clause_link(@current_project, @current_subsection_id, clause)%></li>      <% end %>  </ul>      </div>     