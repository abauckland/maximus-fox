  <div class="current">      <div id="toolbar">        <%= link_to "Back", {:controller => "speclines", :action => "mob_specline_templates", :id => @current_project.id, :subsection_id => @current_subsection_id}, :class => "back", :rel => "external" %>      <h1>select clause to add</h1>      <%= link_to "Finish", {:controller => "projects", :action => "show", :id => @current_project.id, :subsection => @current_subsection_id}, :class => "del", :rel => "external" %>       </div>    <% if @template_project_clauses.blank? %>    <ul>      <li class="arrow_blank">No clauses are available to add</li>        </ul>    <% else %>     <ul>      <%@template_project_clauses.each do |clause| %>        <li  id='<%= clause.id %>' class="add_arrow"><%= link_to "#{clause.clause_section_full_title}", {:controller => "speclines", :action => "mob_add_clause", :id => @current_project.id, :add_clause_id => clause.id, :subsection_id => @current_subsection_id, :template_id => @current_template_project.id}, :class => "get"%></li>          <% end %>    </ul>  <% end %>      </div>     