    <!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"        "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">      <html>        <head>      <%= stylesheet_link_tag 'specright-ff' if !mobile_device?  %>      <%= stylesheet_link_tag 'specright-mobile' if mobile_device? %>         <%= javascript_include_tag   'jquery', 'rails.js', 'jquery.idTabs.min', 'autocomplete-rails.js', 'application' if !mobile_device?  %>      <%= javascript_include_tag   'jquery', 'rails.js', 'mobile' if mobile_device? %>    </head>        <body>   <div class="current">      <div id="toolbar">       <%= link_to "Back", {:controller => "projects", :action => "show", :id => @current_project.id, :subsection => @current_subsection_id}, :class => "back", :rel => "external" %>      <h1>Select Template</h1>     </div>        <% unless flash.empty? %>        <div class="info">        <%- flash.each do |name, msg| -%>          <%= content_tag :div, msg, :id => "flash_#{name}" %>        <%- end -%>        </div>      <% end %>      <ul>  <%@selectable_templates.each do |template| %>  <li class="arrow"><%= link_to "#{template.code} #{template.title}", {:controller => "speclines", :action => "mob_show_clauses", :id => @current_project.id, :subsection_id => @current_subsection_id, :template_id => template.id}%></li>      <% end %>  </ul>      </div>     </body>  </html> 