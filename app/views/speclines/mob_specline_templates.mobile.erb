  <div class="current">      <div id="toolbar">       <%= link_to "Back", {:controller => "projects", :action => "show", :id => @current_project.id, :subsection => @current_subsection_id}, :class => "back", :rel => "external" %>      <h1>Select Template</h1>     </div>        <% unless flash.empty? %>        <div class="info">        <%- flash.each do |name, msg| -%>          <%= content_tag :div, msg, :id => "flash_#{name}" %>        <%- end -%>        </div>      <% end %>      <ul>  <%@selectable_templates.each do |template| %>  <li class="arrow"><%= link_to "#{template.code} #{template.title}", {:controller => "speclines", :action => "mob_show_clauses", :id => @current_project.id, :subsection_id => @current_subsection_id, :template_id => template.id}%></li>      <% end %>  </ul>      </div>     