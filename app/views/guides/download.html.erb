
<%= form_tag(:controller => "guides", :action => "download_guides") do %>
     
	<%= select_tag "guidance_ids", options_from_collection_for_select(@guidance_notes, :id, :guide_full_code_and_title_and_date), :multiple => true, :size => 8 %>    

<%= submit_tag "download", :class=>"edit_submit" %>



<%= form_for @guide do |f| %>
	<%= f.label :pdf, "Guidance Note:" %>
	<%= f.file_field :pdf %>
<%= f.submit "Create Free Account"%>
  