<%= form_for(@supplier) do |f| %>
  <% if @supplier.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@supplier.errors.count, "error") %> prohibited this supplier from being saved:</h2>

      <ul>
      <% @supplier.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div id="supplier_form">
    <p>
    	<label><%= f.label :company %></label>
    	<%= f.text_field :company %>
    </p>
          
	<p
		<label><%= f.label :photo, "Company Logo:" %></label>
		<%= f.file_field :photo  %>
	</p>
    <p>
    	<label><%= f.label :www %></label>
    	<%= f.text_field :www %>
    </p>
   
  </div>
  <div id="supplier_submit">
    <%= f.submit %>
  </div>
<% end %>
